#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/fredhu/Desktop/Items/fred-items/node_modules/.pnpm/vite@5.2.13_@types+node@20.11.5_less@4.2.0_lightningcss@1.24.1_sass@1.75.0/node_modules/vite/bin/node_modules:/Users/fredhu/Desktop/Items/fred-items/node_modules/.pnpm/vite@5.2.13_@types+node@20.11.5_less@4.2.0_lightningcss@1.24.1_sass@1.75.0/node_modules/vite/node_modules:/Users/fredhu/Desktop/Items/fred-items/node_modules/.pnpm/vite@5.2.13_@types+node@20.11.5_less@4.2.0_lightningcss@1.24.1_sass@1.75.0/node_modules:/Users/fredhu/Desktop/Items/fred-items/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/fredhu/Desktop/Items/fred-items/node_modules/.pnpm/vite@5.2.13_@types+node@20.11.5_less@4.2.0_lightningcss@1.24.1_sass@1.75.0/node_modules/vite/bin/node_modules:/Users/fredhu/Desktop/Items/fred-items/node_modules/.pnpm/vite@5.2.13_@types+node@20.11.5_less@4.2.0_lightningcss@1.24.1_sass@1.75.0/node_modules/vite/node_modules:/Users/fredhu/Desktop/Items/fred-items/node_modules/.pnpm/vite@5.2.13_@types+node@20.11.5_less@4.2.0_lightningcss@1.24.1_sass@1.75.0/node_modules:/Users/fredhu/Desktop/Items/fred-items/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/vite@5.2.13_@types+node@20.11.5_less@4.2.0_lightningcss@1.24.1_sass@1.75.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/vite@5.2.13_@types+node@20.11.5_less@4.2.0_lightningcss@1.24.1_sass@1.75.0/node_modules/vite/bin/vite.js" "$@"
fi
