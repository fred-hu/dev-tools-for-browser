#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/fredhu/Desktop/Items/fred-items/node_modules/.pnpm/plasmo@0.88.0_@swc+core@1.4.15_@swc+helpers@0.5.10__@swc+helpers@0.5.10_lodash@4.17.21_postcs_ihj45xamaskzwrkbemymxwufgy/node_modules/plasmo/bin/node_modules:/Users/fredhu/Desktop/Items/fred-items/node_modules/.pnpm/plasmo@0.88.0_@swc+core@1.4.15_@swc+helpers@0.5.10__@swc+helpers@0.5.10_lodash@4.17.21_postcs_ihj45xamaskzwrkbemymxwufgy/node_modules/plasmo/node_modules:/Users/fredhu/Desktop/Items/fred-items/node_modules/.pnpm/plasmo@0.88.0_@swc+core@1.4.15_@swc+helpers@0.5.10__@swc+helpers@0.5.10_lodash@4.17.21_postcs_ihj45xamaskzwrkbemymxwufgy/node_modules:/Users/fredhu/Desktop/Items/fred-items/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/fredhu/Desktop/Items/fred-items/node_modules/.pnpm/plasmo@0.88.0_@swc+core@1.4.15_@swc+helpers@0.5.10__@swc+helpers@0.5.10_lodash@4.17.21_postcs_ihj45xamaskzwrkbemymxwufgy/node_modules/plasmo/bin/node_modules:/Users/fredhu/Desktop/Items/fred-items/node_modules/.pnpm/plasmo@0.88.0_@swc+core@1.4.15_@swc+helpers@0.5.10__@swc+helpers@0.5.10_lodash@4.17.21_postcs_ihj45xamaskzwrkbemymxwufgy/node_modules/plasmo/node_modules:/Users/fredhu/Desktop/Items/fred-items/node_modules/.pnpm/plasmo@0.88.0_@swc+core@1.4.15_@swc+helpers@0.5.10__@swc+helpers@0.5.10_lodash@4.17.21_postcs_ihj45xamaskzwrkbemymxwufgy/node_modules:/Users/fredhu/Desktop/Items/fred-items/node_modules/.pnpm/node_modules:$NODE_PATH"
